{
  "name": "yy-random",
  "version": "1.2.0",
  "description": "a javascript random number API with seeded support. not cryptographically sound. useful for games",
  "main": "random.js",
  "scripts": {
    "test": "node_modules/.bin/mocha",
    "docs": "node_modules/.bin/jsdoc2md --template README.hbs random.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/davidfig/random.git"
  },
  "author": "david figatner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidfig/random/issues"
  },
  "homepage": "https://github.com/davidfig/random#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "expect.js": "^0.3.1",
    "jsdoc-to-markdown": "^1.3.7",
    "mocha": "^3.0.2"
  },
  "eslintConfig": {
    "env":
    {
        "commonjs": true,
        "es6": true
    },
    "parserOptions": {
    },
    "rules": {
        "indent": ["error", 4],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-undef": 1,
        "no-unused-vars": 1
    },
    "globals": {
        "PIXI": true,
        "document": true,
        "window": true,
        "debug": true,
        "debugOne": true,
        "console": true
    }
  }
}
